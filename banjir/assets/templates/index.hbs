<!DOCTYPE html>
<html lang="{{site.locale-url-prefix}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="minimal-ui, width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta name="description" content="Peta Jakarta REM">
    <meta name="author" content="Peta Jakarta">

    <title>{{{site.name}}}</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="/{{site.url-prefix}}/img/petajakarta_icon_32x32.png">

    <!-- Fonts from Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Lato:300,400,900' rel='stylesheet' type='text/css'>

    <!-- Site stylesheet -->
    <link href="/{{site.url-prefix}}/css/application.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
  </head>

  <body class="{{{site.locale-url-prefix}}}">

    <div id="page">
    
      <div id="header">
      
        <!-- Fixed navbar -->
        <div class="navbar navbar-default navbar-fixed-top">
          <div class="container">
            <div class="navbar-header">
              <img src="/{{site.url-prefix}}/img/petajakarta_white_pixel_top.png" width="auto" height="48" alt=""/>
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar" style="background-color:#CCC;"></span>
                <span class="icon-bar" style="background-color:#CCC;"></span>
                <span class="icon-bar" style="background-color:#CCC;"></span>
              </button>
            </div>
            <div class="collapse navbar-collapse navbar-ex1-collapse">
              <ul class="nav navbar-nav navbar-right">
                <li class="active"><a href="/{{site.url-prefix}}/{{site.locale-url-prefix}}/">{{{navbar.rem}}}</a></li>
                <li><a href="/{{site.url-prefix}}/{{site.other-locale-url-prefix}}/">[{{{navbar.other-locale}}}]</a></li>
                <li id="usermenu">
                	<button class="btn btn-default dropdown-toggle" type="button" id="userDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                		<span class="glyphicon glyphicon-user"></span><span class="username">(loading)</span>
                		<span class="caret"></span>
                	</button>
                	<ul class="dropdown-menu" aria-labelledby="userDropdown">
					    <li><a href="/logout">Logout</a></li>
				  	</ul>
                </li>
              </ul>
            </div>
          </div><!-- /.container -->
        </div><!-- /.navbar -->


        <!-- Modal -->
        <div class="modal fade" id="reportsModal" tabindex="-1" role="dialog" aria-labelledby="reportsModal" aria-hidden="true" style="height:500px;">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">{{{confirmed-reports.title}}}</h4>
                {{{confirmed-reports.instructions}}}
              </div>
              <div class="modal-body" id="modal-reports-body" style="height:300px; overflow:scroll;">
              </div>
              <div class="modal-footer">
                <div class="pull-left"><label><input type="checkbox" autocomplete="off" onclick="toggle_timestamp(this.checked);">{{{confirmed-reports.timestamp}}}</label></div>
                <button type="button" class="btn btn-default" data-dismiss="modal">{{modal.close}}</button>
              </div>
            </div><!-- /.modal-content -->
          </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
        
        <!-- Info Modal -->
        <div class="modal fade" id="infoModal" tabindex="-1" role="dialog" aria-labelledby="infoModal" aria-hidden="true" style="height:500px;">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">{{{map-information.title}}}</h4>
              </div>
              <div class="modal-body" id="modal-info-body" style="height:300px; overflow:scroll;">
                {{{map-information.acknowledgements}}}
              </div>
              <div class="modal-footer">
                <h5 class="pull-left">PetaJakarta.org &copy; SMART Infrastructure Facility, University of Wollongong</h5>
                <button type="button" class="btn btn-default" data-dismiss="modal">{{modal.close}}</button>

              </div>
            </div><!-- /.modal-content -->
          </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
        
      </div><!-- /#header -->
      
      <div id="content">
      
        <div id="map"></div>
        
        <div class="dragger" style="width: 100%; background-color: rgb(204, 204, 204); cursor: ns-resize; height: 3px;"></div>
        
        <div id="table-area">
          <table id="table-header" width="100%">
              <thead>
                  <tr class="header">
                      <th width="25"><a class="collapse-all"><span class='glyphicon glyphicon-minus' aria-hidden='true'></span></a></th>
                      <th width="10%">{{state-table.id}}</th>
                      <th width="30%">{{state-table.name}}</th>
                      <th width="20%">{{state-table.reports}}</th>
                      <th width="10%">{{state-table.dims}}</th>
                      <th width="*">{{state-table.actions}}</th>
                  </tr>
              </thead>
          </table>
		  <div id="table-container">
		    <table id="table-data" width="100%">
		      <thead>
		        <tr class="header">
		          <th width="25"></th>
		          <th width="10%"></th>
		          <th width="30%"></th>
		          <th width="20%"></th>
		          <th width="10%"></th>
		          <th width="*"></th>
		        </tr>
		      </thead>
		      <tbody>
		      </tbody>
		    </table>
		  </div>
        </div><!-- /#table-area -->
        
      </div><!-- /#content -->
      
    </div><!-- /#page -->

  <!-- Site JavaScript -->
  <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
  <script src="//code.jquery.com/ui/1.10.4/jquery-ui.js"></script>

  <script> //Determine if device is touchscreen/mobile
    window.isTouch = (('ontouchstart' in window) || (navigator.msMaxTouchPoints > 0));
    L_PREFER_CANVAS = false; //Leaflet canvas rendering
  </script>
  <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
  <script src="/{{site.url-prefix}}/js/application.min.js"></script>
  <script src="/{{site.url-prefix}}/js/map.min.js"></script>

  <!-- Google analytics -->
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-46996603-1', 'petajakarta.org');
    ga('send', 'pageview');
  </script>

</body>
</html>
